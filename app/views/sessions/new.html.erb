<div class="login">
  <div class="container col-md-2 col-md-offset-5">
    <div class="input-group input-group-lg">
      <%= bootstrap_form_for(:session, url: login_path) do |f| %>
        <%= f.email_field :email %>
        <%= f.password_field :password %>
        <%= f.submit "Sign In" %>
      <% end %>
    </div>
  </div>
</div>
